<h1>Add Physician</h1>
<% = form_with(model: physician, local: true, class: "ui form" ) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :email %>
    <%= f.text_field :email %><br><br>

    <%= f.label :specialty %>
    <%= f.text_field :specialty %><br><br>
  </div
  <%= f.submit nil, class: "ui blue button inverted" %>
<% end %>