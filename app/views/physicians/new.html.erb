<h1>Add Physician</h1>
<%= form_with(model: @physician, local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, required: true, autofocus: true, placeholder: "Name" %>
  </div>
  <div class="field">
    <%= f.label :specialty %>
    <%= f.text_field :email, required: true, placeholder: "Specialty" %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email, required: true, placeholder: "Email" %>
  </div>
  <%= f.submit nil, class: "ui green button inverted" %>
<% end %>